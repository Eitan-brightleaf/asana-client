{
  "name": "brightleaf-digital/asana-client",
  "description": "A PHP client library for the Asana API",
  "type": "library",
  "license": "MIT", 
  "authors": [
    {
      "name": "Eitan Adler",
      "email": "eitan@brightleafc.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "league/oauth2-client": "^2.8.1",
    "guzzlehttp/guzzle": "^7.9.3"
  },
  "require-dev": {
    "vlucas/phpdotenv": "v5.6.1",
    "squizlabs/php_codesniffer": "@stable",
    "phpunit/phpunit": "@stable"
  },
  "autoload": {
    "psr-4": {
      "BrightleafDigital\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "BrightleafDigital\\Tests\\": "tests/"
    }
}
}